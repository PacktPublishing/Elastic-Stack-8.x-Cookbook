GET movies/_search
{
  "query": {
    "range": {
      "release_year": {
        "gte": "1925",
        "lte": "1927"
      }
    }
  }
}

GET movies/_search
{
  "query": {
    "multi_match": {
      "query": "come home",
      "fields": [
        "title",
        "plot"
      ]
    }
  }
}

